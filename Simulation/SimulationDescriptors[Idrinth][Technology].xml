<?xml version="1.0" encoding="utf-8" ?>

<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">
    <SimulationDescriptor Name="TechnologyMassProducedInfantry" Type="Technology"/>
    <SimulationDescriptor Name="TechnologyHighlyArmoredInfantry" Type="Technology"/>
    <SimulationDescriptor Name="TechnologyExperimentalWeaponry" Type="Technology"/>
    <SimulationDescriptor Name="EmpireImprovementTechnologyMassProducedInfantry" Type="EmpireImprovement">
        <Modifier Name="InfantryManpowerCost" Operation="Addition" Value="-1" Path="./ClassEmpire"/>
    </SimulationDescriptor>
    <SimulationDescriptor Name="EmpireImprovementTechnologyHighlyArmoredInfantry" Type="EmpireImprovement">
        <Modifier Name="InfantryMaxHealth" Operation="Addition" Value="10" Path="./ClassEmpire"/>
        <Modifier Name="InfantryDamagesMultiplierAgainstTanks" Operation="Percent" Value="0.025" Path="./ClassEmpire"/>
    </SimulationDescriptor>
    <SimulationDescriptor Name="EmpireImprovementTechnologyExperimentalWeaponry" Type="EmpireImprovement">
        <Modifier Name="InfantryMaxDamages" Operation="Percent" Value="0.2" Path="./ClassEmpire"/>
        <Modifier Name="TankMaxDamages" Operation="Percent" Value="0.15" Path="./ClassEmpire"/>
        <Modifier Name="PlaneMaxDamages" Operation="Percent" Value="0.1" Path="./ClassEmpire"/>
        <Modifier Name="InfantryMinDamages" Operation="Percent" Value="-0.2" Path="./ClassEmpire"/>
        <Modifier Name="TankMinDamages" Operation="Percent" Value="-0.15" Path="./ClassEmpire"/>
        <Modifier Name="PlaneMinDamages" Operation="Percent" Value="-0.1" Path="./ClassEmpire"/>
        <Modifier Name="InfantryDamagesMultiplierAgainstInfantry" Operation="Percent" Value="0.05" Path="./ClassEmpire"/>
        <Modifier Name="TankDamagesMultiplierAgainstTank" Operation="Percent" Value="0.05" Path="./ClassEmpire"/>
        <Modifier Name="PlaneDamagesMultiplierAgainstPlane" Operation="Percent" Value="0.05" Path="./ClassEmpire"/>
    </SimulationDescriptor>
</Datatable>

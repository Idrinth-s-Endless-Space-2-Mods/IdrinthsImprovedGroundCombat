    <QuestDefinition Name="QuestIdrinthEmpireTraitCraverAffinity" IsInstant="true" SingleCheckPerTurn="true" Category="General" SubCategory="GroundCombat">
        <Tags>Hidden,BeginTurn</Tags>
        <QuestContextSolo/>
        <RepetitionRules NumberOfOccurrencesPerPlayer="1"/>
        <Vars>
            <Var VarName="$CurrentEmpire">
                <From Source="$Empire"/>
            </Var>
            <Var VarName="$Effect" StringValue="VisualAffinityCravers"/>
        </Vars>
        <Prerequisites Target="$(Empire)">
            <PathPrerequisite Flags="Prerequisite">ClassEmpire, AffinityMappingCravers</PathPrerequisite>
        </Prerequisites>
        <Steps>
            <Step Name="Step1">
                <ObjectiveSet>
                    <Objective Name="IET1_1">
                        <Sequence>
                            <Action_ApplyDescriptor LogAction="true">
                                <Input_DescriptorName VarName="$Effect"/>
                                <Input_Targets VarName="$CurrentEmpire"/>
                            </Action_ApplyDescriptor>
                        </Sequence>
                    </Objective>
                </ObjectiveSet>
            </Step>
        </Steps>
    </QuestDefinition>
